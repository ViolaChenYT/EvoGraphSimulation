Universe = vanilla
Executable = cwrapper.sh
Arguments = $(process)


Error = log/osgJob.$(process).err
Output = log/osgJob.$(process).out
Log = log/osgJob.$(process).log


should_transfer_files = YES
when_to_transfer_output = ON_EXIT
Notification = never
transfer_input_files=parameters.in, main, graphs, results
transfer_output_files = results

requirements = (HAS_CVMFS_oasis_opensciencegrid_org =?= TRUE && HAS_FILE_usr_lib64_libgmp_so_3 == true)
Queue 5000
